{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "published_at": {
      "type": "date",
      "required": true
    },
    "category": {
      "model": "category",
      "via": "articles"
    },
    "hot": {
      "type": "enumeration",
      "enum": [
        "hi",
        "hello"
      ]
    },
    "email": {
      "type": "email"
    },
    "login": {
      "type": "dynamiczone",
      "components": [
        "comm.alert"
      ]
    }
  }
}
